    </validationRules>
    <validationRules>
        <fullName>End_Date_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c  &gt;  End_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>The End date has to be equal or greater than the Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Details_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>(TEXT( Job_Category__c ) == &apos;Other&apos; &amp;&amp; ISBLANK( Details__c )) || 
(TEXT(Job_Category__c) != &apos;Other&apos; &amp;&amp; NOT(ISBLANK(Details__c)))</errorConditionFormula>
        <errorMessage>Please enter the other Job Category</errorMessage>
    </validationRules>
